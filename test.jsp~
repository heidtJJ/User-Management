<%@ taglib uri="http://java.sun.com/jsp/jstl/sql" prefix="sql" %>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>

<sql:update var="is" dataSource="jdbc/TestDB">
INSERT INTO users (name, password) VALUES ("newJared","coolpassword")
</sql:update>

<sql:query var="rs" dataSource="jdbc/TestDB">
select name, password from users
</sql:query>

<html>
  <head>
    <title>DB Test</title>
  </head>
  <body>

  <h2>Results</h2>

<c:forEach var="row" items="${rs.rows}">
    Name: ${row.name} &nbsp Password: ${row.password}<br/>
</c:forEach>

  </body>
</html>
